{
  "compilerOptions": {
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "@screens/*": [
        "src/screens/*"
      ],
      "@config/*": [
        "src/config/*"
      ],
      "@src/*": [
        "src/*"
      ],
      "@components/*": [
        "src/components/*"
      ]
    }
  },
  "extends": "expo/tsconfig.base"
}
