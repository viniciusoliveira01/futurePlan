    "native-base": "^2.13.12",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-42.0.0.tar.gz",
    "react-native-chart-kit": "^6.3.0",
    "react-native-gesture-handler": "~1.10.2",
    "react-native-material-textfield": "^0.16.1",
